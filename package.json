{
  "name": "@kamechan/runway-video-server",
  "version": "0.1.5",
  "description": "A Model Context Protocol server for generating videos from images using RunwayAPI",
  "type": "module",
  "bin": {
    "runway-video-server": "build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "@runwayml/sdk": "^1.4.4",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.17.10",
    "typescript": "^5.3.3"
  }
}
